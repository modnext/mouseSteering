<?xml version="1.0" encoding="utf-8" standalone="no"?>
<GUIProfiles>
  <!-- Base Reference -->
  <Profile name="mouseSteeringSettingsMenuButton" extends="fs25_settingsPauseButton">
    <iconSliceId value="gui.storeAttribute_tireName"/>
  </Profile>

  <!-- Dialog Background -->
  <Profile name="mouseSteeringDialogFullscreenBg" extends="dialogFullscreenBg"/>
  <Profile name="mouseSteeringDialogBgMiddle" extends="fs25_dialogBgMiddle"/>
  <Profile name="mouseSteeringDialogBgTop" extends="fs25_dialogBgTop"/>
  <Profile name="mouseSteeringDialogBgBottom" extends="fs25_dialogBgBottom"/>

  <!-- Dialog Content -->
  <Profile name="mouseSteeringDialogContentContainer" extends="fs25_dialogContentContainer"/>

  <!-- Line Separator -->
  <Profile name="mouseSteeringLineSeparatorBottom" extends="baseReference" with="anchorBottomStretchingX">
    <size value="100% 1dp"/>
    <imageColor value="$preset_fs25_colorGreyLight_20"/>
  </Profile>

  <!-- Progress Bar -->
  <Profile name="mouseSteeringProgressBarBackground" extends="baseReference" with="anchorTopStretchingX pivotTopLeft">
    <size value="100% 6px"/>
    <position value="0px -30px"/>
    <imageColor value="$preset_fs25_colorMainDark"/>
    <imageSliceId value="gui.progressbar_middle"/>
    <startImageSize value="5px 0"/>
    <startImageColor value="$preset_fs25_colorMainDark"/>
    <startImageSliceId value="gui.progressbar_left"/>
    <endImageSize value="5px 0"/>
    <endImageColor value="$preset_fs25_colorMainDark"/>
    <endImageSliceId value="gui.progressbar_right"/>
  </Profile>
  <Profile name="mouseSteeringProgressBar" extends="mouseSteeringProgressBarBackground" with="anchorMiddleStretchingX pivotMiddleLeft">
    <position value="0px 0px"/>
    <imageColor value="$preset_fs25_colorMainHighlight"/>
    <startImageColor value="$preset_fs25_colorMainHighlight"/>
    <endImageColor value="$preset_fs25_colorMainHighlight"/>
  </Profile>
  <Profile name="mouseSteeringProgressBarDanger" extends="mouseSteeringProgressBar">
    <imageColor value="$preset_colorRed"/>
    <startImageColor value="$preset_colorRed"/>
    <endImageColor value="$preset_colorRed"/>
  </Profile>

  <!-- Options -->
  <Profile name="mouseSteeringSettingsContainer" extends="baseReference" with="anchorTopLeft">
    <size value="815px 90px"/>
    <hasFrame value="true"/>
    <frameColor value="transparent"/>
    <frameThickness value="0 0 0 1dp"/>
    <frameBottomColor value="$preset_fs25_colorGreyLight_20"/>
    <imageColor value="$preset_colorTransparent"/>
  </Profile>
  <Profile name="mouseSteeringSettingsTitle" extends="fs25_settingsMultiTextOptionTitle" with="anchorStretchingYLeft">
    <width value="450px"/>
    <position value="20px 12px"/>
    <textSize value="17px"/>
    <textBold value="true"/>
  </Profile>
  <Profile name="mouseSteeringSettingsDescription" extends="fs25_settingsMultiTextOptionTitle" with="anchorStretchingYLeft">
    <width value="450px"/>
    <position value="20px -10px"/>
    <textSize value="16px"/>
    <textBold value="false"/>
    <textMaxNumLines value="1"/>
    <textColor value="$preset_fs25_colorGreyText"/>
  </Profile>
  <Profile name="mouseSteeringSettingsContainerNumLines2" extends="mouseSteeringSettingsContainer">
    <height value="105px"/>
  </Profile>
  <Profile name="mouseSteeringSettingsTitleNumLines2" extends="mouseSteeringSettingsTitle">
    <position value="20px 20px"/>
  </Profile>
  <Profile name="mouseSteeringSettingsDescriptionNumLines2" extends="mouseSteeringSettingsDescription">
    <position value="20px -11px"/>
    <textMaxNumLines value="2"/>
  </Profile>
  <Profile name="mouseSteeringSettingsSectionHeader" extends="fs25_textDefault" with="anchorTopStretchingX">
    <size value="100% 60px"/>
    <textSize value="28px"/>
    <textUpperCase value="true"/>
    <textColor value="$preset_fs25_colorMainHighlight"/>
  </Profile>

  <!-- Slider option -->
  <Profile name="mouseSteeringSliderOption" extends="fs25_multiTextOption" with="anchorMiddleRight">
    <size value="276px 32px"/>
    <margin value="0 0 0 20px"/>
    <position value="-10px 0px"/>
    <sliderOffset value="0px"/>
    <continuousInputStep value="9"/>
    <updateTextPosition value="false"/>
    <hideLeftRightButtons value="false"/>
    <useFillingBar value="true"/>
    <defaultProfileButtonLeft value="mouseSteeringSliderOptionLeft"/>
    <defaultProfileButtonRight value="mouseSteeringSliderOptionRight"/>
    <defaultProfileBgThreePart value="mouseSteeringSliderOptionBg"/>
    <defaultProfileText value="mouseSteeringSliderOptionText"/>
    <defaultTextColor value="$preset_fs25_colorMainLight"/>
    <overlapTextColor value="$preset_fs25_colorMainDark"/>
    <defaultProfileSlider value="mouseSteeringSliderOptionIcon"/>
    <defaultProfileFillingBarThreePart value="mouseSteeringSliderOptionFillingBar"/>
  </Profile>
  <Profile name="mouseSteeringSliderOptionLeft" extends="buttonBase" with="anchorStretchingYLeft">
    <size value="40px 100%"/>
    <position value="-25px 0px"/>
    <clickSound value="NONE"/>
    <sendActionOnRelease value="false"/>
    <iconRotation value="180"/>
    <iconSize value="7px 8px"/>
    <iconTextOffset value="-7px 0px"/>
    <iconSliceId value="gui.3dShop_arrow"/>
    <iconColor value="$preset_colorTransparent"/>
    <iconHighlightedColor value="$preset_fs25_colorGreyLight"/>
    <iconFocusedColor value="$preset_fs25_colorGreyLight"/>
    <iconPressedColor value="$preset_fs25_colorMainHighlight"/>
    <iconDisabledColor value="$preset_colorTransparent"/>
    <imageSliceId value="noSlice"/>
    <imageColor value="0 0 0 0"/>
    <imageHighlightedColor value="0 0 0 0"/>
    <imageFocusedColor value="0 0 0 0"/>
    <imageSelectedColor value="0 0 0 0"/>
    <imageDisabledColor value="0 0 0 0"/>
  </Profile>
  <Profile name="mouseSteeringSliderOptionRight" extends="mouseSteeringSliderOptionLeft" with="anchorStretchingYRight">
    <position value="25px 0px"/>
    <iconRotation value="0"/>
    <iconTextOffset value="7px 0px"/>
  </Profile>
  <Profile name="mouseSteeringSliderOptionBg" extends="baseReference" with="anchorMiddleStretchingX">
    <imageSliceId value="gui.3dShop_button_middle"/>
    <imageColor value="$preset_fs25_colorGrey"/>
    <startImageSize value="6px 0"/>
    <startImageSliceId value="gui.3dShop_button_left"/>
    <startImageColor value="$preset_fs25_colorGrey"/>
    <endImageSize value="6px 0"/>
    <endImageSliceId value="gui.3dShop_button_right"/>
    <endImageColor value="$preset_fs25_colorGrey"/>
  </Profile>
  <Profile name="mouseSteeringSliderOptionFillingBar" extends="baseReference" with="anchorMiddleStretchingX pivotMiddleLeft">
    <imageSliceId value="gui.3dShop_button_middle"/>
    <imageColor value="$preset_fs25_colorGreyBinarySlider"/>
    <imageFocusedColor value="$preset_fs25_colorMainHighlight"/>
    <imageHighlightedColor value="$preset_fs25_colorMainHighlight"/>
    <startImageSize value="6px 0"/>
    <startImageSliceId value="gui.3dShop_button_left"/>
    <startImageColor value="$preset_fs25_colorGreyBinarySlider"/>
    <startImageFocusedColor value="$preset_fs25_colorMainHighlight"/>
    <startImageHighlightedColor value="$preset_fs25_colorMainHighlight"/>
    <endImageSize value="6px 0"/>
    <endImageSliceId value="gui.3dShop_button_right"/>
    <endImageColor value="$preset_fs25_colorGreyBinarySlider"/>
    <endImageFocusedColor value="$preset_fs25_colorMainHighlight"/>
    <endImageHighlightedColor value="$preset_fs25_colorMainHighlight"/>
    <imageDisabledColor value="$preset_fs25_colorGreyBinarySlider_20"/>
    <startImageDisabledColor value="$preset_fs25_colorGreyBinarySlider_20"/>
    <endImageDisabledColor value="$preset_fs25_colorGreyBinarySlider_20"/>
  </Profile>
  <Profile name="mouseSteeringSliderOptionIcon" extends="baseReference" with="anchorStretchingYLeft">
    <width value="0"/>
    <absoluteSizeOffset value="0 0"/>
    <imageColor value="$preset_fs25_colorGreyBinarySlider"/>
    <imageFocusedColor value="$preset_fs25_colorMainHighlight"/>
    <imageHighlightedColor value="$preset_fs25_colorMainHighlight"/>
    <imageDisabledColor value="$preset_fs25_colorGreyBinarySlider_20"/>
  </Profile>
  <Profile name="mouseSteeringSliderOptionText" extends="fs25_multiTextOptionText">
    <position value="0px 0"/>
    <textSize value="16px"/>
  </Profile>

  <!-- Multi text option -->
  <Profile name="mouseSteeringMultiTextOption" extends="fs25_multiTextOption" with="anchorMiddleRight">
    <size value="276px 32px"/>
    <defaultProfileBgThreePart value="mouseSteeringMultiTextOptionBg"/>
    <defaultProfileText value="mouseSteeringMultiTextOptionText"/>
  </Profile>
  <Profile name="mouseSteeringMultiTextOptionBg" extends="fs25_multiTextOptionBg">
    <imageSliceId value="gui.3dShop_button_middle"/>
    <imageColor value="$preset_fs25_colorGrey"/>
    <startImageSize value="6px 0"/>
    <startImageSliceId value="gui.3dShop_button_left"/>
    <startImageColor value="$preset_fs25_colorGrey"/>
    <endImageSize value="6px 0"/>
    <endImageSliceId value="gui.3dShop_button_right"/>
    <endImageColor value="$preset_fs25_colorGrey"/>
  </Profile>
  <Profile name="mouseSteeringMultiTextOptionText" extends="fs25_multiTextOptionText">
    <textColor value="$preset_fs25_colorMainLight"/>
    <textSelectedColor value="$preset_fs25_colorMainDark"/>
    <textFocusedColor value="$preset_fs25_colorMainDark"/>
    <textHighlightedColor value="$preset_fs25_colorMainDark"/>
  </Profile>

  <!-- Settings options -->
  <Profile name="mouseSteeringSettingsSliderOption" extends="mouseSteeringSliderOption" with="anchorMiddleRight">
    <position value="-12px 0px"/>
    <focusOnHighlight value="true"/>
  </Profile>
  <Profile name="mouseSteeringSettingsBinaryOption" extends="fs25_binaryOption" with="anchorMiddleRight">
    <position value="-12px 0px"/>
    <focusOnHighlight value="true"/>
  </Profile>
  <Profile name="mouseSteeringSettingsMultiTextOption" extends="mouseSteeringMultiTextOption">
    <position value="-12px 0px"/>
    <focusOnHighlight value="true"/>
    <wrap value="false"/>
    <hideButtonOnLimitReached value="true"/>
  </Profile>
  <Profile name="mouseSteeringSettingsOptionLocked" extends="emptyPanel" with="anchorMiddleRight">
    <size value="20px 27px"/>
    <imageSliceId value="gui.multiplayer_lock"/>
    <imageColor value="$preset_fs25_colorMainHighlight"/>
    <position value="-300px 0px"/>
  </Profile>

  <!-- Slider box -->
  <Profile name="mouseSteeringListSliderBox" extends="fs25_listSliderBox">
    <position value="-15px 0px"/>
  </Profile>
  <Profile name="mouseSteeringListSlider" extends="fs25_listSlider"/>

  <!-- Dialog buttons -->
  <Profile name="mouseSteeringDialogButtonBox" extends="fs25_dialogButtonBox"/>
  <Profile name="mouseSteeringDialogButtonBoxSeparator" extends="fs25_dialogButtonBoxSeparator"/>

  <!-- Tabs -->
  <Profile name="mouseSteeringTabbed" extends="fs25_subCategorySelector">
    <size value="0 0"/>
    <defaultProfileText value="fs25_subCategorySelectorTabbedText"/>
    <hideLeftRightButtons value="true"/>
  </Profile>
  <Profile name="mouseSteeringTabbedBox" extends="emptyPanel" with="anchorTopStretchingX">
    <height value="40px"/>
    <handleFocus value="false"/>
    <alignmentX value="left"/>
    <alignmentY value="bottom"/>
  </Profile>
  <Profile name="mouseSteeringTabbedTab" extends="emptyPanel">
    <size value="200px 36px"/>
    <handleFocus value="false"/>
    <fitExtraWidth value="40px"/>
    <ignorePressedOverlayState value="true"/>
    <textSize value="16px"/>
    <textBold value="true"/>
    <textUpperCase value="true"/>
    <textAutoWidth value="true"/>
    <textAlignment value="center"/>
    <textOffset value="0px 0"/>
    <textColor value="$preset_fs25_colorMainLight"/>
    <textSelectedColor value="$preset_fs25_colorMainDark"/>
    <textDisabledColor value="$preset_colorDisabled"/>
  </Profile>
  <Profile name="mouseSteeringTabbedTabBg" extends="emptyPanel">
    <handleFocus value="false"/>
    <imageSelectedColor value="$preset_colorWhite"/>
    <imageSelectedSliceId value="gui.tab_selected_middle"/>
    <startImageSize value="12px 0"/>
    <startImageSliceId value="gui.empty"/>
    <startImageSelectedColor value="$preset_colorWhite"/>
    <startImageSelectedSliceId value="gui.tab_selected_left"/>
    <endImageSize value="12px 0"/>
    <endImageSliceId value="gui.empty"/>
    <endImageSelectedColor value="$preset_colorWhite"/>
    <endImageSelectedSliceId value="gui.tab_selected_right"/>
  </Profile>
</GUIProfiles>




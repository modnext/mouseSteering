<?xml version="1.0" encoding="utf-8" standalone="no"?>
<GUI onOpen="onOpen" onClose="onClose" onCreate="onCreate">
  <GuiElement profile="newLayer"/>
  <Bitmap profile="mouseSteeringDialogFullscreenBg"/>

  <GuiElement profile="mouseSteeringSettingsDialogContainer" id="dialogElement">
    <ThreePartBitmap profile="mouseSteeringDialogBgMiddle"/>
    <ThreePartBitmap profile="mouseSteeringDialogBgTop"/>
    <ThreePartBitmap profile="mouseSteeringDialogBgBottom"/>

    <GuiElement profile="mouseSteeringDialogContentContainer" id="contentContainer">
      <!-- Header -->
      <Text profile="mouseSteeringSettingsDialogTitle" id="dialogTitle" text="$l10n_mouseSteering_ui_title"/>
      <GuiElement profile="mouseSteeringControlsButtonContainer" id="controlsButtonContainer">
        <ThreePartBitmap profile="mouseSteeringControlsButtonBgLight"/>
        <Text profile="mouseSteeringControlsButtonText" id="controlsButtonText" text="$l10n_ui_settingsControls"/>
        <Button profile="mouseSteeringControlsButtonAction1" id="controlsButton" onClick="onClickOpenPageSettingsControls"/>
      </GuiElement>
      <ThreePartBitmap profile="mouseSteeringSettingsDialogLineSeparatorTop"/>

      <!-- Sub category selector -->
      <MultiTextOption profile="mouseSteeringSettingsDialogTabSelector" id="pageSelector" onClick="updateSubCategoryPages" buttonLRChange="true"/>
      <BoxLayout profile="mouseSteeringSettingsDialogTabSelectorBox" id="subCategoryBox">
        <Button profile="mouseSteeringSettingsDialogTabSelectorTab" id="subCategoryTabs[1]" onClick="onClickGeneral" text="$l10n_mouseSteering_ui_general">
          <ThreePartBitmap profile="mouseSteeringSettingsDialogTabSelectorTabBg" name="background"/>
        </Button>
        <Button profile="mouseSteeringSettingsDialogTabSelectorTab" id="subCategoryTabs[2]" onClick="onClickHud" text="$l10n_mouseSteering_ui_hud">
          <ThreePartBitmap profile="mouseSteeringSettingsDialogTabSelectorTabBg" name="background"/>
        </Button>
        <Button profile="mouseSteeringSettingsDialogTabSelectorTab" id="subCategoryTabs[3]" onClick="onClickManage" text="$l10n_mouseSteering_ui_dataControls">
          <ThreePartBitmap profile="mouseSteeringSettingsDialogTabSelectorTabBg" name="background"/>
        </Button>
      </BoxLayout>

      <!-- Vehicle status -->
      <Bitmap profile="mouseSteeringSettingsDialogVehicleStatus">
        <GuiElement profile="mouseSteeringSettingsDialogVehicleStatusIndicator" id="vehicleStatusIndicator">
          <Text profile="mouseSteeringSettingsDialogVehicleStatusName" text="$l10n_mouseSteering_ui_vehicleCounter"/>
          <Text profile="mouseSteeringSettingsDialogVehicleStatusFillLevel" name="fillLevel"/>

          <ThreePartBitmap profile="mouseSteeringProgressBarBackground">
            <ThreePartBitmap profile="mouseSteeringProgressBar" name="bar"/>
          </ThreePartBitmap>

          <Button profile="mouseSteeringSettingsDialogVehicleStatusSyncButton" id="syncVehiclesButton" onClick="onClickSyncVehicles" text="$l10n_mouseSteering_button_sync"/>
        </GuiElement>
      </Bitmap>

      <!-- Page: General settings -->
      <GuiElement profile="mouseSteeringSettingsDialogBoxContainer" id="subCategoryPages[1]">
        <ScrollingLayout profile="mouseSteeringSettingsDialogLayout" id="generalSettingsLayout">
          <Bitmap profile="mouseSteeringSettingsContainer">
            <BinaryOption profile="mouseSteeringSettingsBinaryOption" id="default" onClick="onToggleChanged" focusInit="onOpen"/>
            <Text profile="mouseSteeringSettingsTitle" text="$l10n_mouseSteering_ui_default"/>
            <Text profile="mouseSteeringSettingsDescription" text="$l10n_mouseSteering_ui_default_desc"/>
          </Bitmap>
          <Bitmap profile="mouseSteeringSettingsContainer">
            <BinaryOption profile="mouseSteeringSettingsBinaryOption" id="autoSave" onClick="onToggleChanged"/>
            <Text profile="mouseSteeringSettingsTitle" text="$l10n_mouseSteering_ui_autoSave"/>
            <Text profile="mouseSteeringSettingsDescription" text="$l10n_mouseSteering_ui_autoSave_desc"/>
            <Bitmap profile="mouseSteeringSettingsOptionLocked" name="iconDisabled"/>
          </Bitmap>
          <Text profile="mouseSteeringSettingsSectionHeader" name="sectionHeader" text="$l10n_mouseSteering_ui_controlAttributes"/>
          <Bitmap profile="mouseSteeringSettingsContainer">
            <MouseSteeringSliderOption profile="mouseSteeringSettingsSliderOption" id="sensitivity" onClick="onSliderChanged" minValue="0.10" stepSize="0.10" maxValue="4"/>
            <Text profile="mouseSteeringSettingsTitle" text="$l10n_mouseSteering_ui_sensitivity"/>
            <Text profile="mouseSteeringSettingsDescription" text="$l10n_mouseSteering_ui_sensitivity_desc"/>
          </Bitmap>
          <Bitmap profile="mouseSteeringSettingsContainer">
            <BinaryOption profile="mouseSteeringSettingsBinaryOption" id="invertXAxis" onClick="onToggleChanged"/>
            <Text profile="mouseSteeringSettingsTitle" text="$l10n_mouseSteering_ui_invertXAxis"/>
            <Text profile="mouseSteeringSettingsDescription" text="$l10n_mouseSteering_ui_invertXAxis_desc"/>
          </Bitmap>
          <Bitmap profile="mouseSteeringSettingsContainerNumLines2">
            <BinaryOption profile="mouseSteeringSettingsBinaryOption" id="speedBasedSteering" onClick="onToggleChanged"/>
            <Text profile="mouseSteeringSettingsTitleNumLines2" text="$l10n_mouseSteering_ui_speedBasedSteering"/>
            <Text profile="mouseSteeringSettingsDescriptionNumLines2" text="$l10n_mouseSteering_ui_speedBasedSteering_desc"/>
          </Bitmap>
          <Bitmap profile="mouseSteeringSettingsContainerNumLines2">
            <MouseSteeringSliderOption profile="mouseSteeringSettingsSliderOption" id="linearity" onClick="onSliderChanged" minValue="0.10" stepSize="0.05" maxValue="3"/>
            <Text profile="mouseSteeringSettingsTitleNumLines2" text="$l10n_mouseSteering_ui_linearity"/>
            <Text profile="mouseSteeringSettingsDescriptionNumLines2" text="$l10n_mouseSteering_ui_linearity_desc"/>
            <Bitmap profile="mouseSteeringSettingsOptionLocked" name="iconDisabled"/>
          </Bitmap>
          <Bitmap profile="mouseSteeringSettingsContainer">
            <MouseSteeringSliderOption profile="mouseSteeringSettingsSliderOption" id="smoothness" onClick="onSliderChanged" minValue="0" stepSize="0.05" maxValue="1"/>
            <Text profile="mouseSteeringSettingsTitle" text="$l10n_mouseSteering_ui_smoothness"/>
            <Text profile="mouseSteeringSettingsDescription" text="$l10n_mouseSteering_ui_smoothness_desc"/>
          </Bitmap>
          <Bitmap profile="mouseSteeringSettingsContainerNumLines2">
            <MouseSteeringSliderOption profile="mouseSteeringSettingsSliderOption" id="deadzone" onClick="onSliderChanged" minValue="0" stepSize="0.05" maxValue="1"/>
            <Text profile="mouseSteeringSettingsTitleNumLines2" text="$l10n_mouseSteering_ui_deadzone"/>
            <Text profile="mouseSteeringSettingsDescriptionNumLines2" text="$l10n_mouseSteering_ui_deadzone_desc"/>
          </Bitmap>
          <Text profile="mouseSteeringSettingsSectionHeader" name="sectionHeader" text="$l10n_mouseSteering_ui_steeringAssist"/>
          <Bitmap profile="mouseSteeringSettingsContainer">
            <BinaryOption profile="mouseSteeringSettingsBinaryOption" id="steeringAssist" onClick="onToggleChanged"/>
            <Text profile="mouseSteeringSettingsTitle" text="$l10n_mouseSteering_ui_steeringAssistMode"/>
            <Text profile="mouseSteeringSettingsDescription" text="$l10n_mouseSteering_ui_steeringAssistMode_desc"/>
          </Bitmap>
          <Bitmap profile="mouseSteeringSettingsContainerNumLines2">
            <BinaryOption profile="mouseSteeringSettingsBinaryOption" id="steeringAssistLockout" onClick="onToggleChanged"/>
            <Text profile="mouseSteeringSettingsTitleNumLines2" text="$l10n_mouseSteering_ui_steeringAssistLockout"/>
            <Text profile="mouseSteeringSettingsDescriptionNumLines2" text="$l10n_mouseSteering_ui_steeringAssistLockout_desc"/>
            <Bitmap profile="mouseSteeringSettingsOptionLocked" name="iconDisabled"/>
          </Bitmap>
          <Bitmap profile="mouseSteeringSettingsContainer">
            <MouseSteeringSliderOption profile="mouseSteeringSettingsSliderOption" id="steeringAssistThreshold" onClick="onSliderChanged" minValue="0" stepSize="0.05" maxValue="1"/>
            <Text profile="mouseSteeringSettingsTitle" text="$l10n_mouseSteering_ui_steeringAssistThreshold"/>
            <Text profile="mouseSteeringSettingsDescription" text="$l10n_mouseSteering_ui_steeringAssistThreshold_desc"/>
            <Bitmap profile="mouseSteeringSettingsOptionLocked" name="iconDisabled"/>
          </Bitmap>
        </ScrollingLayout>
      </GuiElement>

      <!-- Page: HUD settings -->
      <GuiElement profile="mouseSteeringSettingsDialogBoxContainer" id="subCategoryPages[2]">
        <ScrollingLayout profile="mouseSteeringSettingsDialogLayout" id="hudSettingsLayout">
          <Bitmap profile="mouseSteeringSettingsContainer">
            <MouseSteeringMultiTextOption profile="mouseSteeringSettingsMultiTextOption" id="indicatorMode" onClick="onMultiChanged" texts="$l10n_mouseSteering_ui_both|$l10n_mouseSteering_ui_inside|$l10n_mouseSteering_ui_outside|$l10n_ui_off"/>
            <Text profile="mouseSteeringSettingsTitle" text="$l10n_mouseSteering_ui_indicatorMode"/>
            <Text profile="mouseSteeringSettingsDescription" text="$l10n_mouseSteering_ui_indicatorMode_desc"/>
          </Bitmap>
          <Bitmap profile="mouseSteeringSettingsContainer">
            <BinaryOption profile="mouseSteeringSettingsBinaryOption" id="indicatorText" onClick="onToggleChanged"/>
            <Text profile="mouseSteeringSettingsTitle" text="$l10n_mouseSteering_ui_indicatorText"/>
            <Text profile="mouseSteeringSettingsDescription" text="$l10n_mouseSteering_ui_indicatorText_desc"/>
          </Bitmap>
          <Bitmap profile="mouseSteeringSettingsContainer">
            <BinaryOption profile="mouseSteeringSettingsBinaryOption" id="indicatorLookBackInside" onClick="onToggleChanged"/>
            <Text profile="mouseSteeringSettingsTitle" text="$l10n_mouseSteering_ui_indicatorLookBackInside"/>
            <Text profile="mouseSteeringSettingsDescription" text="$l10n_mouseSteering_ui_indicatorLookBackInside_desc"/>
          </Bitmap>
        </ScrollingLayout>
      </GuiElement>

      <!-- Page: Manage vehicles -->
      <GuiElement profile="mouseSteeringSettingsDialogBoxContainer" id="subCategoryPages[3]">
        <BoxLayout profile="mouseSteeringSettingsDialogLayout" id="manageSettingsLayout">
          <!-- Filter -->
          <MultiTextOption profile="mouseSteeringTabbed" id="filterSelector" onClick="updateFilterPages"/>
          <BoxLayout profile="mouseSteeringTabbedBox" id="filterBox">
            <Button profile="mouseSteeringTabbedTab" id="filterSelectorTab[1]" text="$l10n_mouseSteering_ui_all" onClick="onClickFilterAll">
              <ThreePartBitmap profile="mouseSteeringTabbedTabBg" name="background"/>
            </Button>
            <Button profile="mouseSteeringTabbedTab" id="filterSelectorTab[2]" text="$l10n_mouseSteering_ui_saved" onClick="onClickFilterSaved">
              <ThreePartBitmap profile="mouseSteeringTabbedTabBg" name="background"/>
            </Button>
            <Button profile="mouseSteeringTabbedTab" id="filterSelectorTab[3]" text="$l10n_mouseSteering_ui_unsaved" onClick="onClickFilterUnsaved">
              <ThreePartBitmap profile="mouseSteeringTabbedTabBg" name="background"/>
            </Button>
          </BoxLayout>

          <ThreePartBitmap profile="mouseSteeringLineSeparatorBottom"/>
          <GuiElement profile="mouseSteeringVehiclesDialogHeaderBox">
            <Text profile="mouseSteeringVehiclesDialogHeaderText" text="$l10n_mouseSteering_ui_nameVehicle"/>
            <Text profile="mouseSteeringVehiclesDialogHeaderText" position="452px 0" textAlignment="right" text="$l10n_mouseSteering_ui_plateVehicle"/>
            <ThreePartBitmap profile="mouseSteeringLineSeparatorBottom"/>
          </GuiElement>

          <SmoothList profile="mouseSteeringVehiclesDialogList" id="vehiclesList" onDoubleClick="onDoubleClickVehiclesListItem">
            <ListItem profile="mouseSteeringVehiclesDialogListItem">
              <ThreePartBitmap profile="mouseSteeringVehiclesDialogListItemBg" name="alternating"/>
              <Bitmap profile="mouseSteeringVehiclesDialogListItemIcon" name="icon"/>
              <Text profile="mouseSteeringVehiclesDialogListItemName" name="name"/>
              <Text profile="mouseSteeringVehiclesDialogListItemInfo" name="licensePlate"/>
            </ListItem>
          </SmoothList>
        </BoxLayout>

        <BoxLayout profile="mouseSteeringVehiclesDialogLegendBox" id="vehiclesLegend">
          <Bitmap profile="mouseSteeringVehiclesDialogLegendIcon" imageSliceId="mouseSteering.icon_checkmark"/>
          <Text profile="mouseSteeringVehiclesDialogLegendName" text="$l10n_mouseSteering_ui_saved"/>
          <Bitmap profile="mouseSteeringVehiclesDialogLegendIcon" imageSliceId="mouseSteering.icon_ring"/>
          <Text profile="mouseSteeringVehiclesDialogLegendName" text="$l10n_mouseSteering_ui_unsaved"/>
        </BoxLayout>

        <BoxLayout profile="mouseSteeringVehiclesDialogSelectionBox">
          <Text profile="mouseSteeringVehiclesDialogSelectionName" id="vehiclesSelectionName" text="$l10n_mouseSteering_ui_vehiclesSelectionName"/>
        </BoxLayout>

        <GuiElement profile="mouseSteeringVehiclesDialogDetailsBox" id="vehicleDetailBox" newLayer="true">
          <ThreePartBitmap profile="mouseSteeringVehiclesDialogDetailsBoxBg"/>

          <Bitmap profile="mouseSteeringVehiclesDialogDetailsImageBox">
            <Bitmap profile="mouseSteeringVehiclesDialogDetailsImage" id="vehicleDetailImage"/>
          </Bitmap>

          <InGameMapPreview profile="mouseSteeringVehiclesDialogDetailsIngameMap" id="vehicleDetailMap"/>
          <Bitmap profile="mouseSteeringVehiclesDialogDetailsIngameMapMask"/>
          <Button profile="mouseSteeringVehiclesDialogDetailsButtonAction1" id="viewOnMapButton" onClick="onVehicleViewOnMap" text="$l10n_mouseSteering_button_viewOnMap"/>

          <BoxLayout profile="mouseSteeringVehiclesDialogDetailsInfoBox" id="vehicleDetailAttributesLayout">
            <GuiElement profile="mouseSteeringVehiclesDialogDetailsInfoItem">
              <Bitmap profile="mouseSteeringVehiclesDialogDetailsInfoIcon" imageSliceId="gui.storeAttribute_maintenanceCost"/>
              <Text profile="mouseSteeringVehiclesDialogDetailsInfoText" id="status"/>
            </GuiElement>
            <GuiElement profile="mouseSteeringVehiclesDialogDetailsInfoItem">
              <Bitmap profile="mouseSteeringVehiclesDialogDetailsInfoIcon" imageSliceId="gui.storeAttribute_licensePlate"/>
              <Text profile="mouseSteeringVehiclesDialogDetailsInfoText" id="licensePlate"/>
            </GuiElement>
            <GuiElement profile="mouseSteeringVehiclesDialogDetailsInfoItem">
              <Bitmap profile="mouseSteeringVehiclesDialogDetailsInfoIcon" imageSliceId="gui.storeAttribute_condition"/>
              <Text profile="mouseSteeringVehiclesDialogDetailsInfoText" id="damage"/>
            </GuiElement>
            <GuiElement profile="mouseSteeringVehiclesDialogDetailsInfoItem">
              <Bitmap profile="mouseSteeringVehiclesDialogDetailsInfoIcon" imageSliceId="gui.storeAttribute_operatingHours"/>
              <Text profile="mouseSteeringVehiclesDialogDetailsInfoText" id="operatingTime"/>
            </GuiElement>
            <GuiElement profile="mouseSteeringVehiclesDialogDetailsInfoItem">
              <Bitmap profile="mouseSteeringVehiclesDialogDetailsInfoIcon" imageSliceId="gui.storeAttribute_time"/>
              <Text profile="mouseSteeringVehiclesDialogDetailsInfoText" id="lastSaved"/>
            </GuiElement>
          </BoxLayout>
        </GuiElement>

        <Text profile="mouseSteeringVehiclesDialogListEmptyText" id="vehicleDetailEmptyText" text="$l10n_mouseSteering_ui_vehiclesListEmpty"/>
      </GuiElement>

      <ThreePartBitmap profile="mouseSteeringListSliderBox" position="-15px -35px" absoluteSizeOffset="0 150px">
        <Slider profile="mouseSteeringListSlider" id="settingsSlider" dataElementId="generalSettingsLayout"/>
      </ThreePartBitmap>
    </GuiElement>

    <!-- Footer -->
    <Text profile="mouseSteeringSettingsDialogModeSettings" id="modeSettings" text="$l10n_mouseSteering_ui_modeSettingsLabel"/>
    <Text profile="mouseSteeringSettingsDialogModeSettingsValue" id="modeSettingsValue" text="$l10n_mouseSteering_ui_singleplayer"/>
    <BoxLayout profile="mouseSteeringDialogButtonBox" id="buttonsBox">
      <Button profile="buttonActivate" id="viewDetailButton" text="$l10n_mouseSteering_button_viewDetails" onClick="onClickViewDetail">
        <Bitmap profile="mouseSteeringDialogButtonBoxSeparator"/>
      </Button>
      <Button profile="buttonOK" id="toggleButton" text="$l10n_mouseSteering_button_save" onClick="onClickToggle">
        <Bitmap profile="mouseSteeringDialogButtonBoxSeparator"/>
      </Button>
      <Button profile="buttonExtra1" id="resetButton" text="$l10n_button_defaults" onClick="onClickReset">
        <Bitmap profile="mouseSteeringDialogButtonBoxSeparator"/>
      </Button>
      <Button profile="buttonBack" id="backButton" text="$l10n_mouseSteering_button_saveBack" onClick="onClickBack" />
    </BoxLayout>
  </GuiElement>

  <GUIProfiles>
    <Profile name="mouseSteeringSettingsDialogContainer" extends="emptyPanel" with="anchorMiddleCenter">
